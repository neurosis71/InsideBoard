namespace Insideboard\DbBundle\Document;

use Doctrine\ODM\MongoDB\Mapping\Annotations as MongoDB;

/**
 * @MongoDB\Document
 */
class Grade 
{
    /**
     * @MongoDB\Id
     */
    private $id;
    
    /**
     * @MongoDB\Field(type="string")
     */
    private $serial;
    
    /**
     * @MongoDB\Field(type="string")
     */
    private $label;
    
    /** 
     * @MongoDB\EmbedOne(targetDocument="GradeCategory") 
     */
    private $gradeCategory;
    
}